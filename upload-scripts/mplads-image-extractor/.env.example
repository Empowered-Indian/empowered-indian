# Database Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database

# Cloudflare R2 Configuration
R2_ACCESS_KEY_ID=your_r2_access_key
R2_SECRET_ACCESS_KEY=your_r2_secret_key
R2_ENDPOINT=https://your_account_id.r2.cloudflarestorage.com
R2_REGION=auto
R2_BUCKET_NAME=mplads-images
R2_PUBLIC_DOMAIN=https://your_custom_domain.com

# MPLADS API Configuration
MPLADS_BASE_URL=https://mplads.sbi
MPLADS_SESSION_COOKIE=your_session_cookie_here

# Processing Configuration (Render optimized)
BATCH_SIZE=10
RATE_LIMIT_DELAY=3000
MAX_CONCURRENT_WORKERS=1
ENABLE_THUMBNAILS=false
PROGRESS_SUMMARY_INTERVAL=100